{
  "name": "nest-file",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nest-file/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/nest-file",
        "tsConfig": "libs/nest-file/tsconfig.lib.json",
        "packageJson": "libs/nest-file/package.json",
        "main": "libs/nest-file/src/index.ts",
        "assets": ["libs/nest-file/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/nest-file/**/*.ts",
          "libs/nest-file/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/nest-file/jest.config.ts"
      }
    }
  },
  "tags": []
}
